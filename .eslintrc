{
    extends: "eslint:recommended",
    "env": {
        "browser": true,
        "es6": true
    },
    "rules": {
        "strict": [2, "global"],
        "no-console": 1,
        "no-unused-vars": [2, {"vars": "all", "args": "none"}]
    },
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module"
    }
}
